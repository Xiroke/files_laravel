	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"pm.request.addHeader({\r",
					"    key: 'Accept',\r",
					"    value: 'application/json'\r",
					"})\r",
					"\r",
					"const token = pm.environment.get('auth_token')\r",
					"\r",
					"if (token && token != '') {\r",
					"    pm.request.headers.remove('Authorization');\r",
					"    \r",
					"    pm.request.addHeader({\r",
					"        key: 'Authorization',\r",
					"        value: `Bearer ${token}`\r",
					"    })\r",
					"}"
				]
			}
